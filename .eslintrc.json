{
  "plugins": ["@typescript-eslint", "eslint-plugin-prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/order": "off",
    "semi": "off",
    "quotes": "off",
    "@typescript-eslint/quotes": ["error", "single"],
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/consistent-type-imports": "off"
  }
}
